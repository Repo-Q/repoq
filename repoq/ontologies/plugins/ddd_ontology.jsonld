  "@graph": [
    {
      "@id": "ddd:DomainConcept",
      "@type": "owl:Class",
      "rdfs:label": "Domain Concept",
      "rdfs:comment": "Base class for all Domain-Driven Design concepts"
    },
    {
      "@id": "ddd:BoundedContext",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Bounded Context", 
      "rdfs:comment": "A linguistic boundary within which a particular domain model is valid"
    },
    {
      "@id": "ddd:Aggregate",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Aggregate",
      "rdfs:comment": "A cluster of associated objects treated as a unit for data changes"
    },
    {
      "@id": "ddd:AggregateRoot",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:Entity",
      "rdfs:label": "Aggregate Root",
      "rdfs:comment": "The single entry point to an aggregate"
    },
    {
      "@id": "ddd:Entity", 
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Entity",
      "rdfs:comment": "An object with identity and lifecycle"
    },
    {
      "@id": "ddd:ValueObject",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Value Object",
      "rdfs:comment": "An object without identity, defined by its attributes"
    },
    {
      "@id": "ddd:DomainService",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Domain Service",
      "rdfs:comment": "A service that encapsulates domain logic"
    },
    {
      "@id": "ddd:Repository",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept", 
      "rdfs:label": "Repository",
      "rdfs:comment": "Encapsulates logic for accessing data sources"
    },
    {
      "@id": "ddd:DomainEvent",
      "@type": "owl:Class",
      "rdfs:subClassOf": "ddd:DomainConcept",
      "rdfs:label": "Domain Event", 
      "rdfs:comment": "Something that happened in the domain that domain experts care about"
    },
    
    {
      "@id": "ddd:belongsToContext",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": "ddd:DomainConcept",
      "rdfs:range": "ddd:BoundedContext", 
      "rdfs:label": "belongs to context",
      "rdfs:comment": "Indicates which bounded context a concept belongs to"
    },
    {
      "@id": "ddd:implementedBy",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": "ddd:DomainConcept",
      "rdfs:range": "code:CodeElement",
      "rdfs:label": "implemented by",
      "rdfs:comment": "Links domain concepts to their code implementation"
    },
    {
      "@id": "ddd:mappedToComponent",
      "@type": "owl:ObjectProperty", 
      "rdfs:domain": "ddd:BoundedContext",
      "rdfs:range": "c4:Component",
      "rdfs:label": "mapped to component",
      "rdfs:comment": "Links bounded contexts to C4 components"
    },
    
    {
      "@id": "ddd:publishes",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": "ddd:DomainConcept",
      "rdfs:range": "ddd:DomainEvent",
      "rdfs:label": "publishes",
      "rdfs:comment": "Indicates that a concept publishes domain events"
    },
    {
      "@id": "ddd:handles",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": "ddd:DomainService",
      "rdfs:range": ["ddd:Command", "ddd:Query"],
      "rdfs:label": "handles",
      "rdfs:comment": "Indicates that a service handles commands or queries"
    }
  ]
