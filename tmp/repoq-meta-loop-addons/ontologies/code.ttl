@prefix code: <https://field33.com/ontologies/code/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

code:Module a owl:Class ; rdfs:label "Module" .
code:Class  a owl:Class ; rdfs:label "Class" .
code:Function a owl:Class ; rdfs:label "Function" .

code:hasComplexity a owl:DatatypeProperty ;
  rdfs:domain code:Function ;
  rdfs:range xsd:integer ;
  rdfs:comment "Cyclomatic complexity" .

code:imports a owl:ObjectProperty ;
  rdfs:domain code:Module ;
  rdfs:range  code:Module ;
  rdfs:comment "Module import relation" .
