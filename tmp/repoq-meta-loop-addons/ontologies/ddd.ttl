@prefix ddd: <https://field33.com/ontologies/ddd/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .

ddd:BoundedContext a owl:Class ; rdfs:label "Bounded Context" .
ddd:Entity         a owl:Class ; rdfs:label "Entity" .
ddd:ValueObject    a owl:Class ; rdfs:label "Value Object" .

ddd:mapsToContext  a owl:ObjectProperty ;
  rdfs:domain ddd:BoundedContext ; rdfs:range ddd:BoundedContext .
